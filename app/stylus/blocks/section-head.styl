.section-head
	position relative
	padding-top 49px
	padding-bottom 53px
	background-repeat no-repeat
	background-position center
	background-size cover
	z-index 1
	@media $tPortraitMax
		padding-top 31px
		padding-bottom 27px
	@media $mLandscapeMax
		padding-top 32px
		padding-bottom 36px
	&__container
		@extend .container
		@media $mLandscapeMax
			display grid
	&__breadcrumbs
		margin-bottom 25px
	&__title
		max-width 670px
		margin-bottom 32px
		font-size 36px
		line-height 48px
		@media $desktopMax
			margin-bottom 35px
			line-height 43px
		@media $tPortraitMax
			margin-bottom 20px
			font-size 32px
			line-height 38px
		@media $mLandscapeMax
			margin-bottom 24px
			font-size 20px
			line-height 24px
		strong
			font-weight 600
	&__rating
		display none
		align-items center
		margin-bottom 28px
		@media $mLandscapeMax
			display flex
		&:before
			@extend .psElem
			width 90px
			height 13px
			margin-right 10px
			background url('data:image/png;base64,') no-repeat left center
	&__rating-text
		color #a3a3a3
		font-size 16px
		font-weight 400
		line-height 16px
		strong
			color #3c3736
			font-weight 600
		a
			color $blue
			line-height 16px
			border-bottom 1px solid $blue
			transition border $duration
			&:hover
				border-bottom-color transparent
	&__text
		position relative
		padding-left 45px
		max-width 668px
		margin-bottom 36px
		color $default
		font-size 18px
		line-height 22px
		@media $tPortraitMax
			margin-bottom 23px
			font-size 16px
			line-height 19px
		@media $mLandscapeMax
			margin-bottom 25px
			padding-left 0
			font-size 14px
			line-height 17px
		&:before
			@extend .psElem
			position absolute
			left 0
			top 10px
			width 27px
			height 2px
			background-color $accent
			@media $mLandscapeMax
				position relative
				display inline-block
				vertical-align top
				top auto
				margin-top 8px
				margin-right 10px
	&__info
		max-width 630px
		margin-bottom 35px
		@media $mLandscapeMax
			display none
	&__info-items
		display grid
		grid-template-columns repeat(3, minmax(156px, auto))
		grid-template-rows auto
		gap 24px 42px
	&__info-item
		position relative
		padding-left 40px
		font-size 14px
		line-height 17px
		&:before
			@extend .psElem
			position absolute
			top 4px
			left 3px
			width 18px
			height 16px
			background url('data:image/png;base64,') no-repeat center
			background-size contain
	&__info-item-title,
	&__info-item-text
		color #3a3835
	&__info-item-title
		font-weight bold
	&__buttons
		display flex
		align-items center
		margin-bottom 33px
		@media $tPortraitMax
			margin-bottom 27px
		@media $mLandscapeMax
			display block
			order 2
			margin-bottom 0
	&__callback-btn
		display block
		margin-right 38px
		padding 24px 63px
		background-color #262626
		border-radius 36px
		box-shadow -3.6px 3.5px 10px rgba(38,38,38,0.35), inset 0 0 12px 6px rgba(255,255,255,0.27)
		transition background $duration, box-shadow $duration
		user-select none
		@media $mLandscapeMax
			max-width 324px
			margin-right 0
			margin-bottom 12px
			padding 24px
			text-align center
		&:hover
			background-color $accent
			box-shadow -3.6px 3.5px 10px rgba(38,38,38, 0), inset 0 0 12px 6px rgba(255, 255, 255, 0)
		span
			text-shadow -1.4px 1.4px 0 rgba(233,65,52,0.3)
			color white
			font-size 14px
			font-weight 600
			line-height 16px
			text-transform uppercase
			white-space nowrap
			@media $mLandscapeMax
				font-size 12px
	&__watch-video-btn
		display flex
		align-items center
		user-select none
		i
			display flex
			justify-content center
			align-items center
			position relative
			width 81px
			height 81px
			background-color rgba(#e5e4e3, .5)
			border-radius 40px
			box-shadow inset 0 0 11.2px 0.8px rgba(255, 255, 255, 0.13)
			z-index 1
			@media $mLandscapeMax
				width 72px
				height 72px
			&:before,
			&:after
				@extend .psElem
			&:before
				width 56px
				height 56px
				background-color #e94134
				border-radius 28px
				box-shadow -3.6px 3.5px 10px rgba(233,65,52,0.35), inset 0 0 11.2px 0.8px rgba(255, 255, 255, 0.27)
				@media $mLandscapeMax
					width 49px
					height 49px
			&:after
				position absolute
				top 50%
				left 50%
				width 16px
				height 16px
				margin-left 2px
				background url('data:image/png;base64,') no-repeat center
				transform translate(-50%, -50%)
				z-index 1
				@media $mLandscapeMax
					width 14px
					height 14px
					margin-left 0
		span
			display -webkit-box
			-webkit-line-clamp 2
			-webkit-box-orient vertical
			max-width 220px
			height 56px
			margin-left -33px
			padding 14px 16px 14px 48px
			color #8c8a86
			font-size 12px
			line-height 14px
			text-overflow ellipsis
			background-color $white
			border-radius 0 28px 28px 0
			overflow hidden
			@media $mLandscapeMax
				max-width 286px
				padding-left 57px
			@media $mPortraitIphone6Max
				max-width calc(100% - 48px)
	&__advantages
		display grid
		grid-template-columns repeat(3, minmax(194px, auto))
		grid-template-rows auto
		gap 24px 48px
		width 100%
		max-width 680px
		@media $tPortraitMax
			grid-template-columns auto
			max-width 324px
			gap 10px
		@media $mLandscapeMax
			order 1
			margin-bottom 21px
	&__advantage
		display flex
		@media $tPortraitMax
			align-items center
			padding 10px 24px
			background-color #fff
			border-radius 30px
		@media $mLandscapeMax
			align-items flex-start
			padding 7px 15px 11px 15px
	&__advantage-overico
		display flex
		flex-direction column
		justify-content center
		align-items center
		width 42px
		height 42px
		flex-shrink 0
		margin-right 20px
	&__advantage-ico
		max-width 100%
		max-height 100%
	&__advantage-text
		padding-top 4px
		font-size 12px
		line-height 14px
	&__img
		position absolute
		right calc((100% - 1164px) / 2 - 144px)
		bottom 0
		width 580px
		height 100%
		background-repeat no-repeat
		background-position center bottom
		background-size auto
		z-index -1
		@media $desktopMax
			right calc((100% - 994px) / 2 - 280px)
		@media $mLandscapeMax
			display none
	&--one
		min-height 414px
		padding 40px 0 34px
		background-color #fbfbfb
		@media $tPortraitMax
			min-height auto
		& ^[0]__grid
			display grid
			grid-template-columns minmax(auto, 650px) auto
			gap 24px
			@media $mLandscapeMax
				grid-template-columns auto
				gap 0
		& ^[0]__cell
			&--decor
				@media $tPortraitMax
					display none
		& ^[0]__title
			margin-bottom 16px
			color $default
			font-size 30px
			line-height 48px
			font-weight 400
			@media $mLandscapeMax
				font-size 20px
				line-height 24px
			strong
				font-weight 600
		& ^[0]__text
			margin-bottom 42px
			padding-left 0
			color #767676
			font-size 14px
			line-height 17px
			font-weight 400
			&:last-child
				margin-bottom 0
			&:before
				content none
				display none
		& ^[0]__buttons
			margin-bottom 0
		& ^[0]__callback-btn
			padding-left 97px
			padding-right 97px
			margin-bottom 0
			@media $mLandscapeMax
				max-width 324px
				padding 24px
		& ^[0]__overimg
			display flex
			justify-content flex-end
			align-items center
			position relative
			max-width 100%
			min-height 350px
			padding-right 15px
			padding-bottom 8px
			text-align right
			&:before
				@extend .psElem
				position absolute
				top 50%
				right 0
				width 304px
				height 304px
				margin-top 4px
				border-radius 50%
				background-color #fff3e0
				transform translateY(calc(-50% - 4px))
				z-index -2
		& ^[0]__img
			position relative
			width auto
			height auto
			right auto
			bottom auto
			max-width 100%
			z-index auto
